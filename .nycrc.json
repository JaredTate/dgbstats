{
  "all": true,
  "include": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "exclude": [
    "src/tests/**",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/setupTests.js",
    "src/reportWebVitals.js",
    "src/index.js",
    "e2e/**",
    "node_modules/**",
    "build/**",
    "public/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": true,
  "branches": 95,
  "lines": 95,
  "functions": 95,
  "statements": 95,
  "watermarks": {
    "lines": [95, 98],
    "functions": [95, 98],
    "branches": [95, 98],
    "statements": [95, 98]
  }
}